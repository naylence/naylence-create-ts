{
  "name": "@naylence/create",
  "version": "0.1.0",
  "description": "CLI to scaffold Naylence starter templates",
  "author": "Naylence Dev <naylencedev@gmail.com>",
  "license": "Apache-2.0",
  "type": "module",
  "keywords": [
    "naylence",
    "cli",
    "templates",
    "scaffold",
    "starter",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/naylence/naylence-create-ts"
  },
  "bugs": {
    "url": "https://github.com/naylence/naylence-create-ts/issues"
  },
  "homepage": "https://github.com/naylence/naylence-create-ts#readme",
  "bin": {
    "naylence-create": "dist/cli.js"
  },
  "main": "dist/index.js",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "scripts": {
    "dev": "tsx src/cli.ts",
    "build": "tsup",
    "start": "node dist/cli.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .tmp"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "fs-extra": "^11.2.0",
    "prompts": "^2.4.2",
    "picocolors": "^1.1.1",
    "tar": "^7.4.3"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.10.2",
    "@types/prompts": "^2.4.9",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
